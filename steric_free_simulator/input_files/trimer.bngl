begin parameters
    konAB 100
    konAC 10
    konBC .01
    koff .01
end parameters

begin species
    A(a,b) 1000
    B(a,b) 1000
    C(a,b) 1000
end species

begin rules
    A(a)+B(a)<->A(a!1).B(a!1) konAB, 0
    A(b)+C(b)<->A(b!1).C(b!1) konAC, 0
    B(b)+C(a)<->B(b!1).C(a!1) konBC, koff
end rules