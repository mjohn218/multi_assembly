begin parameters
    default_assoc   1.
    creation_rate   1e-2
    destruction_rate 1e-3
end parameters

begin species
    A(a,b) 100
    B(a,b) 100
    C(a,b) 100
end species

begin rules
    null<->A(a) G=0
    A(a)<->null G=0
    A(a)+B(a)<->A(a!1).B(a!1) G=-20
    A(b)+C(b)<->A(b!1).C(b!1) G=-20
    B(b)+C(a)<->B(b!1).C(a!1) G=-20
end rules
